		liquidMailSSLPort $
		liquidAdminPort $
		liquidRedoLogProvider
	))
	
################
# liquidMimeEntry is used to represent status about mime types
#
#
################
objectclass ( liquidMimeEntry
	NAME 'liquidMimeEntry'
	DESC 'MIME type info'
	SUP top STRUCTURAL
	MUST (
		cn $
		liquidMimeType $ 
		liquidMimeIndexingEnabled)
	MAY (description $
		liquidMimeFileExtension $
		liquidMimeHandlerClass)
	))

################
# liquidObjectEntry is used to represent status about object types
#
#
################
objectclass ( liquidObjectEntry
	NAME 'liquidObjectEntry'
	DESC 'Object type info'
	SUP top STRUCTURAL
	MUST (
		cn $
		liquidObjectType $ 
		liquidObjectIndexingEnabled $
		liquidObjectStoreMatched)
	MAY (description $
		liquidObjectHandlerConfig $
		liquidObjectHandlerClass)
	))	

################
# liquidTimeZone is used to define a timezone with daylight savings time
