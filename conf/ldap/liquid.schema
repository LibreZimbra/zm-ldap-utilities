#################################################################
# object class definitions
#
# This is where to define the object classes. Object classes are used to define which
# attribute MAY (optional) or MUST (required) belong to an entry. 
#
# classes can be AUXILIARY or STRUCTURAL. An entry in the directory must have one and only one
# structural class, but can have many AUXILIARY classes.
#
# should probably try and keep the attributes listed in MAY and MUST sorted for easy maintenance.
#
#################################################################

################
# liquidMailRecipient is used to represent entries in the directory
# that can receive mail. i.e., they have a visible external address, that
# gets expanded into one or more internal/external addresses.
################

objectclass ( liquidMailRecipient
	NAME 'liquidMailRecipient'
	DESC 'Account object'
	SUP top AUXILIARY
	MAY (	
		liquidMailForwardingAddress $
		liquidMailHost $
		liquidMailAlias $
		liquidMailAddress $
		liquidMailDeliveryAddress $
		liquidMailCanonicalAddress $
		liquidMailCatchAllAddress $
		liquidMailCatchAllForwardingAddress $
		liquidMailCatchAllCanonicalAddress $
		liquidMailStatus) )

################
# liquidAccount extends liquidMailRecipient. It represents a real account in the system
# (either admin or end-user) that can be logged into, etc.
#
# It is defined as AUXILIARY in case it needs to be mixed to an existing directory deployment.
#
# cn            - full name, common name
# co            - country friendly name
# company       - company (company name)
# displayName   - name to display in admin tool, outlook uses as well (cn is multi-valued)
# gn            - first name (given name)
# initials      - middle initial
# l             - city (locality)
# ou            - organizational unit
# physicalDeliveryOfficeName - office
# postalAddress - street address
# postalCode    - zip code
# sn            - last name (sir name)
# st            - state
# telephoneNumber - phone
#
################

objectclass ( liquidAccount 
	NAME 'liquidAccount'
	DESC 'Account object'
	SUP liquidMailRecipient AUXILIARY
	MUST (	
		liquidId $ 
		liquidAccountStatus $
		uid)
	MAY (	
		c $
		co $
		company $
		cn $	
		displayName $
		gn $		
		description $
		initials $
		l $
		liquidAttachmentsBlocked $
		liquidAttachmentsIndexingEnabled $
		liquidAttachmentsViewInHtmlOnly $
		liquidAuthTokenLifetime $
		liquidAdminAuthTokenLifetime $
		liquidContactMaxNumEntries $
		liquidCOSId $
		liquidFeatureContactsEnabled $
		liquidFeatureGalEnabled $	
		liquidFeatureHtmlComposeEnabled $
		liquidFeatureCalendarEnabled $
		liquidFeatureTaggingEnabled $
		liquidFeatureAdvancedSearchEnabled $
		liquidFeatureSavedSearchesEnabled $
		liquidFeatureConversationsEnabled $
		liquidFeatureChangePasswordEnabled $
		liquidFeatureInitialSearchPreferenceEnabled $
		liquidFeatureFiltersEnabled $
		liquidImapEnabled $
		liquidIsAdminAccount $ 
		liquidLastLogonTimestamp $
		liquidMailIdleSessionTimeout $
		liquidMailMessageLifetime $
		liquidMailMinPollingInterval $
		liquidMailQuota $
		liquidMailSieveScript $	
		liquidMailSpamLifetime $
		liquidMailTrashLifetime $
		liquidNewMailNotificationFrom $
		liquidNewMailNotificationSubject $
		liquidNewMailNotificationBody $
		liquidNotes $ 
