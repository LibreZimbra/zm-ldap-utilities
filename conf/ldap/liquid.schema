#
################
objectclass ( liquidTimeZone
	NAME 'liquidTimeZone'
	DESC 'Time Zone info'
	SUP top STRUCTURAL
	MUST (
    	cn $
		liquidTimeZoneStandardDtStart $
		liquidTimeZoneStandardOffset)
	MAY (
		liquidTimeZoneStandardRRule $
		liquidTimeZoneDaylightDtStart $
		liquidTimeZoneDaylightOffset $
		liquidTimeZoneDaylightRRule
	))
